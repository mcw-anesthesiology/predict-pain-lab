    "stylelint": "^7.0.0",
    "stylelint-config-standard": "^11.0.0",
    "velocity-animate": "^1.2.3",
    "webpack": "2.1.0-beta.27",
    "webpack-bundle-analyzer": "^1.5.3",
    "yaml-loader": "^0.4.0"
  }
